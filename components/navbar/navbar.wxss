/* components/navbar/navbar.wxss */
/* 自定义组件 */
.navbar, .navbar view {
	box-sizing: content-box;
}
.navbar .demo{
  position: fixed;
  width: 100vw;
  display: flex;
  align-items: center;
  justify-content: center;
  z-index: 999999;
}
.navbar .demo .left{
  float: left;
  position: absolute;
  width: max-content;
  height: max-content;
  top: 0;
  bottom: 0;
  left: 0;
  margin: auto;
  padding: 16rpx;
}